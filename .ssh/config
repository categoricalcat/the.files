# Docker access via SSH tunneling
Host fufud
  HostName fufuwuqi.local
  User fufud
  Port 24212
  LocalForward 2375 /var/run/docker.sock
  # Optional: Keep connection alive
  ServerAliveInterval 60
  ServerAliveCountMax 3

Host workd
  HostName fufuwuqi.local
  User workd
  Port 24212
  LocalForward 2375 /var/run/docker.sock
  # Optional: Keep connection alive
  ServerAliveInterval 60
  ServerAliveCountMax 3